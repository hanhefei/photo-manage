/**app.wxss**/
page{
    background: #F4F4F4;
    width: 100%;
}
.container {
    overflow: hidden;
    width: 100%;
    height: 100%;
} 


/*隐藏滚动条*/
::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

button::after {
    border: none;
}

.prevBox_model {
    overflow: hidden;
    position: relative;
}
.prevBox_model > image {
    position: absolute;
}
.prevBox_model > view {
    position: absolute;
    width: 100%;
    height: 100%;
}
.prevBox_model > view image {
    position: absolute;
    top: 0;
    left: 0;
    display: block;
    width: 100%;
    height: 100%;
    /* background: url('/resources/imgs/frameBg.png'); */
    /* background-size: cover; */
    /* background-repeat: no-repeat; */
    /* background-position: center; */
}